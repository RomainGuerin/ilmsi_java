<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.e3in.java.controller.MainViewController">
    <top>
        <MenuBar>
            <Menu text="File">
                <MenuItem text="Load File" onAction="#handleLoadFile"/>
            </Menu>
            <Menu text="About">
                <items>
                    <MenuItem text="Infos" onAction="#handleInfos"/>
                </items>
            </Menu>
        </MenuBar>
    </top>
    <center>
        <TableView fx:id="tableView"/>
    </center>
</BorderPane>
